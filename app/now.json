{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": { "cache-control": "s-maxage=31536000,immutable" },
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "$1",
      "continue": true
    }
  ]
}
