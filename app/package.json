    "@sanity/client": "^2.15.0",
    "@sanity/image-url": "^0.140.22",
    "@sentry/browser": "^6.11.0",
    "@sentry/integrations": "^6.11.0",
    "@sentry/node": "^6.11.0",
    "@xstyled/styled-components": "^2.4.1",
    "@zeit/next-source-maps": "^0.0.3",
    "atob": "^2.1.2",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.1",
    "graphql-tag": "^2.12.5",
    "html-parser-lite": "^0.2.2",
    "isomorphic-unfetch": "^3.1.0",
    "js-cookie": "^3.0.0",
    "json2csv": "^5.0.6",
    "next": "^11.1.0",
    "node-fetch": "^2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-player": "^2.9.0",
    "zod": "^3.7.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.1.1",
    "@graphql-codegen/fragment-matcher": "3.1.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/typescript": "2.1.0",
    "@graphql-codegen/typescript-operations": "2.1.0",
    "@graphql-codegen/typescript-react-apollo": "3.1.0",
    "@next/bundle-analyzer": "^11.1.0",
    "@sentry/webpack-plugin": "^1.17.1",
    "@types/atob": "^2.1.2",
    "@types/debug": "^4.1.7",
    "@types/hls.js": "^0.13.3",
    "@types/js-cookie": "^2.2.7",
    "@types/json2csv": "^5.0.3",
    "@types/node-fetch": "^2.5.12",
    "@types/nodemon": "^1.19.1",
    "@types/prettier": "^2.3.2",
    "@types/ramda": "^0.27.44",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/rimraf": "^3.0.2",
    "@types/zeit__next-source-maps": "^0.0.2",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-react-svg": "^3.0.3",
    "babel-plugin-styled-components": "1.13.2",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "graphql-cli": "^4.1.0",
    "jest": "^27.0.6",
    "jest-dom": "^4.0.0",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
